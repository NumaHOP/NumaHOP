env:
  browser: true
extends: 
  - angular
  - eslint:recommended
globals:
  $: true
  _: true
  JSOG: true
  moment: true
  naturalSort: true
  ObjectTools: true
  saveAs: true,
  SockJS: true,
  Stomp: true,
  Uint8Array: true
rules:
  curly: 1
  eqeqeq: 2
  no-alert: 2
  no-eq-null: 1
  no-use-before-define: 0
  semi: 1
  strict: 1
  angular/controller-as-vm: "off"
  angular/controller-name: "off"