<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="pgcn-lot-provider" author="progilone">

        <addColumn tableName="lot_lot">
            <column name="provider" type="varchar(191)"></column>
        </addColumn>

        <addForeignKeyConstraint constraintName="fk_lot_provider"
                                 baseTableName="lot_lot" baseColumnNames="provider"
                                 referencedTableName="user_user" referencedColumnNames="identifier"/>
    </changeSet>

</databaseChangeLog>
